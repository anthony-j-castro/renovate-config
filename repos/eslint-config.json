{
  "extends": [
    "local>anthony-j-castro/renovate-config",
    ":semanticCommitTypeAll(build)"
  ],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "matchPackageNames": ["typescript"],
      "rangeStrategy": "bump"
    },
    {
      "matchDepTypes": ["peerDependencies"],
      "matchPackageNames": ["typescript"],
      "rangeStrategy": "replace"
    }
  ]
}
