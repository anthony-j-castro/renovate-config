{
  "extends": [
    "config:recommended",
    ":disableDependencyDashboard",
    ":rebaseStalePrs",
    ":semanticCommitTypeAll(chore)"
  ],
  "timezone": "America/New_York",
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies", "peerDependencies"],
      "rangeStrategy": "bump",
      "semanticCommitScope": "dev-deps"
    }
  ]
}
